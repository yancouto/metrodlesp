{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2020",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "dist",
    "types": [
      "node",
      "gtag.js"
    ]
  },
  "include": ["src"],
  "exclude": ["dist"]
}
